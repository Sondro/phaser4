    "url": "https://github.com/phaserjs/phaser4/issues"
  },
  "homepage": "https://phaser.io",
  "devDependencies": {
    "chokidar": "^3.0.2",
    "rollup": "^1.20.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript2": "^0.24.0",
    "tslint": "^5.19.0",
    "typescript": "^3.6.2"
