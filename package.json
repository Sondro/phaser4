    "watch-es": "rollup --watch --config rollup.config.es.js",
    "build-umd": "rollup --config rollup.config.umd.js",
    "watch-umd": "rollup --watch --config rollup.config.umd.js",
    "build-iife": "rollup --config rollup.config.iife.js",
    "watch-iife": "rollup --watch --config rollup.config.iife.js"
  },
